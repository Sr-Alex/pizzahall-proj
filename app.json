{
	"expo": {
		"name": "pizzahall-proj",
		"slug": "pizzahall-proj",
		"version": "0.0.2",
		"orientation": "portrait",
		"icon": "./public/icon.png",
		"userInterfaceStyle": "light",
		"scheme": "pizzahall-proj",
		"splash": {
			"image": "./public/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#F5BE14"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.sweetn64.pizzahallproj",
			"config": {
				"usesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./public/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.sweetn64.pizzahallproj"
		},
		"web": {
			"favicon": "./public/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			"@react-native-google-signin/google-signin"
		],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "c8de2577-72ed-4e5a-a1ac-6546aea7d72b"
			}
		}
	}
}
